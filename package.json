{
  "name": "@adobe/parcel-plugin-htl",
  "version": "2.1.1",
  "repository": "https://github.com/adobe/parcel-plugin-htl",
  "description": "Compiles HTL templates to JS functions",
  "main": "src/index.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "start": "node src/run.js",
    "test": "nyc --reporter=text --reporter=lcov --check-coverage mocha",
    "test-ci": "npm run lint && npm run test && codecov",
    "lint": "./node_modules/.bin/eslint .",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@adobe/htlengine": "^2.1.2",
    "fs-extra": "^7.0.1",
    "logform": "^2.1.2",
    "parcel-bundler": "1.12.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.5",
    "ajv": "^6.10.0",
    "codecov": "^3.3.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.2",
    "ghooks": "^2.0.4",
    "js-yaml": "^3.13.0",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "mocha-junit-reporter": "^1.21.0",
    "nyc": "^13.3.0",
    "semantic-release": "^15.13.3",
    "snyk": "^1.147.2"
  },
  "snyk": true,
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
